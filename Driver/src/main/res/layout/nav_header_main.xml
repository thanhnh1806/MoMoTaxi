<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:weightSum="1010">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="239"
        android:background="#7f000000"
        android:orientation="vertical"
        android:paddingLeft="4dp"
        android:paddingRight="14.67dp"
        android:weightSum="771">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="633"
            android:weightSum="138">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:layout_width="114.67dp"
                    android:layout_height="66dp"
                    android:layout_gravity="center"
                    android:src="@drawable/icon_banner" />
            </FrameLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="138"
            android:orientation="vertical"
            android:weightSum="633">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="111"
                android:orientation="vertical"
                android:weightSum="522">

                <FrameLayout
                    android:id="@+id/smRequestTaxi"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="65.25">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:weightSum="423">

                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="332">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:src="@drawable/icon_taxi" />
                        </FrameLayout>

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="91"
                            android:gravity="center_vertical|left"
                            android:text="@string/request_taxi"
                            android:textColor="@android:color/white"
                            android:textSize="8pt" />
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1.33dp"
                        android:layout_gravity="bottom"
                        android:background="#565555" />
                </FrameLayout>

                <FrameLayout
                    android:id="@+id/smMyProfile"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="65.25">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:weightSum="423">

                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="332">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:src="@drawable/icon_myprofile" />
                        </FrameLayout>

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="91"
                            android:gravity="center_vertical|left"
                            android:text="@string/my_profile"
                            android:textColor="@android:color/white"
                            android:textSize="8pt" />
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1.33dp"
                        android:layout_gravity="bottom"
                        android:background="#565555" />
                </FrameLayout>

                <FrameLayout
                    android:id="@+id/smHistory"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="65.25">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:weightSum="423">

                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="332">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:src="@drawable/icon_history" />
                        </FrameLayout>

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="91"
                            android:gravity="center_vertical|left"
                            android:text="@string/history"
                            android:textColor="@android:color/white"
                            android:textSize="8pt" />
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1.33dp"
                        android:layout_gravity="bottom"
                        android:background="#565555" />
                </FrameLayout>

                <FrameLayout
                    android:id="@+id/smFavouriteLocation"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="65.25">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:weightSum="423">

                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="332">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:src="@drawable/icon_favouritelocation" />
                        </FrameLayout>

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="91"
                            android:gravity="center_vertical|left"
                            android:text="@string/favourite_location"
                            android:textColor="@android:color/white"
                            android:textSize="8pt" />
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1.33dp"
                        android:layout_gravity="bottom"
                        android:background="#565555" />
                </FrameLayout>

                <FrameLayout
                    android:id="@+id/smFavouriteDriver"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="65.25">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:weightSum="423">

                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="332">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:src="@drawable/icon_favouritedriver" />
                        </FrameLayout>

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="91"
                            android:gravity="center_vertical|left"
                            android:text="@string/favourite_driver"
                            android:textColor="@android:color/white"
                            android:textSize="8pt" />
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1.33dp"
                        android:layout_gravity="bottom"
                        android:background="#565555" />
                </FrameLayout>

                <FrameLayout
                    android:id="@+id/smReceipt"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="65.25">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:weightSum="423">

                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="332">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:src="@drawable/icon_receipt" />
                        </FrameLayout>

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="91"
                            android:gravity="center_vertical|left"
                            android:text="@string/receipt"
                            android:textColor="@android:color/white"
                            android:textSize="8pt" />
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1.33dp"
                        android:layout_gravity="bottom"
                        android:background="#565555" />
                </FrameLayout>

                <FrameLayout
                    android:id="@+id/smFinishWork"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="65.25">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:weightSum="423">

                    </LinearLayout>
                </FrameLayout>

                <FrameLayout
                    android:id="@+id/smCreditCardSetting"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="65.25">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:weightSum="423">

                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="332">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:src="@drawable/icon_usd" />
                        </FrameLayout>

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="91"
                            android:gravity="center_vertical|left"
                            android:text="@string/credit_card_setting"
                            android:textColor="@android:color/white"
                            android:textSize="8pt" />
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1.33dp"
                        android:layout_gravity="bottom"
                        android:background="#565555" />
                </FrameLayout>

                <FrameLayout
                    android:id="@+id/smCompanyUser"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="65.25">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:weightSum="423">

                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="332">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:src="@drawable/icon_companyuser" />
                        </FrameLayout>

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="91"
                            android:gravity="center_vertical|left"
                            android:text="@string/company_user"
                            android:textColor="@android:color/white"
                            android:textSize="8pt" />
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1.33dp"
                        android:layout_gravity="bottom"
                        android:background="#565555" />
                </FrameLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/smSNSSetting"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="522"
                android:orientation="vertical"
                android:weightSum="2">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="top"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:weightSum="423">

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="332">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:src="@drawable/icon_settings" />
                    </FrameLayout>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="91"
                        android:gravity="center_vertical|left"
                        android:text="@string/sns_setting"
                        android:textColor="@android:color/white"
                        android:textSize="8pt" />
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="771">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#bf000000"
            android:orientation="vertical"
            android:paddingLeft="29.33dp"
            android:paddingRight="29.33dp"
            android:weightSum="771">

            <FrameLayout
                android:id="@+id/smSendFeedback"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="257">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:weightSum="335">

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="283">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical|left"
                            android:src="@drawable/icon_talk" />
                    </FrameLayout>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="52"
                        android:gravity="center_vertical|left"
                        android:text="@string/send_feedback"
                        android:textColor="#999999"
                        android:textSize="8pt" />
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1.33dp"
                    android:layout_gravity="bottom"
                    android:background="#565555" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/smAboutCabby"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="257">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:weightSum="335">

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="283">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical|left"
                            android:src="@drawable/icon_file" />
                    </FrameLayout>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="52"
                        android:gravity="center_vertical|left"
                        android:text="@string/about_cabby"
                        android:textColor="#999999"
                        android:textSize="8pt" />
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1.33dp"
                    android:layout_gravity="bottom"
                    android:background="#565555" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/smTermOfUser"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="257">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:weightSum="335">

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="283">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical|left"
                            android:src="@drawable/icon_news" />
                    </FrameLayout>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="52"
                        android:gravity="center_vertical|left"
                        android:text="@string/terms_of_use"
                        android:textColor="#999999"
                        android:textSize="8pt" />
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1.33dp"
            android:layout_gravity="top"
            android:background="#565555" />
    </FrameLayout>

</LinearLayout>
